{
  "name": "transee",
  "productName": "Transee",
  "version": "2.0.2",
  "description": "Simple and useful tool for quick translation",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack/react.webpack.js --mode development",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --mode production --config webpack/electron.webpack.js",
    "build:react": "webpack --mode production --config webpack/react.webpack.js",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --mac",
    "delete": "rimraf dist && rimraf packages",
    "test": "jest"
  },
  "keywords": [],
  "author": "Alessio Forte <fortalessio@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alessioforte/transee.git"
  },
  "homepage": "https://alessioforte.github.io/transee/",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@commitlint/core": "^9.0.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/jest": "^26.0.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "3.2.0",
    "dotenv": "^8.2.0",
    "electron": "^13.6.6",
    "electron-builder": "^22.8.0",
    "electron-settings": "^4.0.2",
    "electron-updater": "^4.3.4",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "internet-available": "^1.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "wait-on": "^5.2.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "build": {
    "productName": "Transee",
    "appId": "com.alessioforte.transee",
    "directories": {
      "output": "packages"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "alessioforte",
        "repo": "transee"
      }
    ],
    "mac": {
      "icon": "assets/transee.icns",
      "category": "utility.productivity.reference",
      "publish": "github"
    },
    "win": {
      "icon": "assets/transee.ico",
      "publish": "github"
    },
    "files": [
      "package.json",
      "dist/**",
      "assets/**"
    ]
  }
}
